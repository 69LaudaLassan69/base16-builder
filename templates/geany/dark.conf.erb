[theme_info]
name=Base16 <%= @scheme %> Dark
description=Base16 provides carefully chosen syntax highlighting and a default set of sixteen colors suitable for a wide range of applications.
version=0.1
author=Rob Loach (http://robloach.net)
url=http://github.com/chriskempson/base16

[named_styles]
default=#<%= @base["07"]["hex"] %>;#<%= @base["01"]["hex"] %>;false;false
error=#<%= @base["08"]["hex"] %>

# Editor Styles
# =============
selection=#<%= @base["04"]["hex"] %>;#<%= @base["03"]["hex"] %>;false;true
current_line=;#<%= @base["02"]["hex"] %>;true;false
brace_good=#<%= @base["0B"]["hex"] %>;;true;false
brace_bad=#<%= @base["08"]["hex"] %>;;true;false
margin_line_number=#<%= @base["07"]["hex"] %>;#<%= @base["02"]["hex"] %>;false;false
margin_folding=#<%= @base["07"]["hex"] %>;;false;false
fold_symbol_highlight=#<%= @base["01"]["hex"] %>
indent_guide=#<%= @base["02"]["hex"] %>;;false;false
caret=#<%= @base["05"]["hex"] %>;;false;false
marker_line=#<%= @base["07"]["hex"] %>;#ff0;false;false
marker_search=#<%= @base["07"]["hex"] %>;#b8f4b8;false;false
marker_mark=#<%= @base["07"]["hex"] %>;#b8f4b8;
call_tips=#<%= @base["04"]["hex"] %>;;false;false
white_space=;;true

# Programming languages
# =====================
comment=#<%= @base["04"]["hex"] %>
comment_doc=comment
comment_line=comment
comment_line_doc=comment_doc
comment_doc_keyword=comment_doc,bold
comment_doc_keyword_error=comment_doc,italic
number=#<%= @base["0B"]["hex"] %>
number_1=#<%= @base["09"]["hex"] %>
number_2=#<%= @base["09"]["hex"] %>
type=#<%= @base["0A"]["hex"] %>;;false
# Person.rb = Person
class=#<%= @base["0A"]["hex"] %>;;true
# Person.rb = initialize
# PHP
function=#<%= @base["0D"]["hex"] %>;;true;false
parameter=#<%= @base["06"]["hex"] %>

keyword=#<%= @base["0F"]["hex"] %>;;true;false
# Person.rb = class, include, def, end, etc
# PHP = include_once(), if elseif else
keyword_1=#<%= @base["0E"]["hex"] %>;;true;false
keyword_2=#<%= @base["0C"]["hex"] %>;;false;true
keyword_3=#<%= @base["0F"]["hex"] %>;;false;true
keyword_4=#<%= @base["09"]["hex"] %>;;false;true

identifier=#<%= @base["0C"]["hex"] %>;;true;false
# Person.rb = ActiveModel::Conversion
identifier_1=#<%= @base["0A"]["hex"] %>;;true;false
identifier_2=#<%= @base["0C"]["hex"] %>;;true;false
identifier_3=#<%= @base["0D"]["hex"] %>;;true;false
identifier_4=#<%= @base["0E"]["hex"] %>;;true;false

string=#<%= @base["09"]["hex"] %>
string_1=#<%= @base["0B"]["hex"] %>
string_2=#<%= @base["0A"]["hex"] %>
string_3=#<%= @base["0C"]["hex"] %>
string_4=#<%= @base["0D"]["hex"] %>
string_eol=#<%= @base["0E"]["hex"] %>;;false;true

character=string_1
backtick=#<%= @base["0A"]["hex"] %>
here_doc=#<%= @base["0A"]["hex"] %>
scalar=#<%= @base["0A"]["hex"] %>
label=#<%= @base["07"]["hex"] %>;;true;false
# Person.rb = :name
# PHP = $_GET
preprocessor=#<%= @base["0B"]["hex"] %>
regex=number_1
operator=#<%= @base["05"]["hex"] %>;;true;false
decorator=#<%= @base["0D"]["hex"] %>;;false;false
other=#<%= @base["08"]["hex"] %>

# Markup-type languages
# =====================
tag=#<%= @base["0A"]["hex"] %>;;false;false
tag_unknown=#<%= @base["07"]["hex"] %>;#<%= @base["08"]["hex"] %>;true;false
tag_end=#<%= @base["0D"]["hex"] %>;;true;false
attribute=#<%= @base["09"]["hex"] %>;;false;false
attribute_unknown=#<%= @base["07"]["hex"] %>;#<%= @base["08"]["hex"] %>;false;false
value=#<%= @base["0D"]["hex"] %>
entity=#<%= @base["0A"]["hex"] %>

# Diff
# ====
line_added=#<%= @base["0B"]["hex"] %>
line_removed=#<%= @base["08"]["hex"] %>
line_changed=#<%= @base["0D"]["hex"] %>
